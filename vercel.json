{
  "version": 2,
  "builds": [
    {
      "src": "public/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/doge-unblocker",
      "dest": "/public/doge-unblocker.html"
    },
    {
      "src": "/holy-unblocker",
      "dest": "/public/holy-unblocker.html"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    }
  ]
}
