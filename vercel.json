{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/doge-unblocker", "dest": "/doge-unblocker.html" },
    { "src": "/holy-unblocker", "dest": "/holy-unblocker.html" },
    { "src": "/net-shield", "dest": "/net-shield.html" },
    { "src": "/Information", "dest": "/Information.html" }
  ]
}